---
layout: null
permalink: /errorsearch.json
---
[
  {% for error in site.support %}
    {
      "title"    : "{{ support.title | escape }}",
      "category" : "{{ support.category }}",
      "tags"     : "{{ support.tags | join: ', ' }}",
      "url"      : "{{ support.external_url | default: error.url | relative_url }}",
      "date"     : "{{ support.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]