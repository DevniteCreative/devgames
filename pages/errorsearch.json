---
layout: null
permalink: /errorsearch.json
---
[
  {% for error in site.errors %}
    {
      "title"    : "{{ error.title | escape }}",
      "category" : "{{ error.category }}",
      "tags"     : "{{ error.tags | join: ', ' }}",
      "url"      : "{{ error.external_url | default: error.url | relative_url }}",
      "date"     : "{{ error.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]